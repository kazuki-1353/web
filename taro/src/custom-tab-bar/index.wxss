.list {
  display: flex;
  justify-content: space-between;
  align-items: center;

  color: #000;
  background: #fff;
}

.item {
  box-sizing: border-box;

  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;

  padding: 30px 10px 20px;
  height: 130px;
  font-size: 20px;
}

.icon {
  flex: none;
  width: 46px;
  height: 46px;
  pointer-events: none;
}
.iconBig {
  width: 100%;
  height: 100%;
}
.text {
  pointer-events: none;
}
