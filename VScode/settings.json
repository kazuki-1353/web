{
  "update.mode": "manual",
  "update.showReleaseNotes": false,
  "update.enableWindowsBackgroundUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "telemetry.telemetryLevel": "off",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Solarized Light",
  "security.workspace.trust.untrustedFiles": "open",
  "files.eol": "\n",
  "files.associations": {
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.cjson": "jsonc",
    "*.svg": "html"
  },
  "files.exclude": { "**/.ice": true },
  "search.exclude": {
    "**/yarn.lock": true,
    "**/.yaml": true,
    "**/.umi/**": true,
    "**/.next/**": true,
    "**/mock/**": true,
    "**/mock.*": true,
    "**/*.svg": true,
    "**/dist": true
  },
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.parameterHints.enabled": false,
  "editor.suggestSelection": "recentlyUsed",
  "vsintellicode.modify.editor.suggestSelection": "disabled",
  "diffEditor.ignoreTrimWhitespace": true,
  "emmet.includeLanguages": { "javascript": "javascriptreact", "wxml": "html" },
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.arrowParens": "always",
  "prettier.endOfLine": "lf",
  "eslint.format.enable": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "code-runner.runInTerminal": true,
  "java.jdt.ls.java.home": "D:\\Program Files\\Java\\jdk-20",
  "java.maven.downloadSources": true,
  "java.eclipse.downloadSources": true,
  "java.compile.nullAnalysis.mode": "automatic",
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.configuration.maven.globalSettings": "D:\\Program Files\\Java\\apache-maven-3.9.6\\conf\\settings.xml",
  "java.configuration.maven.userSettings": "D:\\Program Files\\Java\\apache-maven-3.9.6\\conf\\settings.xml",
  "maven.executable.path": "D:\\Program Files\\Java\\apache-maven-3.9.6\\bin\\mvn",
  "rsp-ui.rsp.java.home": "D:\\Program Files\\Java\\jdk-20",
  "rsp-ui.enableStartServerOnActivation": [
    {
      "id": "redhat.vscode-community-server-connector",
      "name": "Community Server Connector",
      "startOnActivation": true
    }
  ],
  "vetur.format.defaultFormatterOptions": {
    "prettier": { "singleQuote": true }
  },
  "wxmlConfig.format": { "indent_with_tabs": false, "indent_char": " " },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[java]": { "editor.defaultFormatter": "mwpb.java-prettier-formatter" },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" }
}
