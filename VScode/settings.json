{
  "update.mode": "manual",
  "update.showReleaseNotes": false,
  "update.enableWindowsBackgroundUpdates": false,
  "telemetry.telemetryLevel": "off",
  "extensions.autoUpdate": false,
  "window.menuBarVisibility": "toggle",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "Solarized Light",
  "workbench.iconTheme": "vscode-icons",
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": { "javascript": "javascriptreact", "wxml": "html" },
  "explorer.confirmDelete": false,
  "security.workspace.trust.untrustedFiles": "open",
  "files.eol": "\n",
  "files.associations": {
    "*.wxml": "wxml",
    "*.wxss": "css",
    "*.wxs": "javascript",
    "*.cjson": "jsonc",
    "*.svg": "html"
  },
  "files.exclude": { "**/.ice": true },
  "search.exclude": {
    "**/.umi/**": true,
    "**/dist": true,
    "**/mock.*": true,
    "**/mock/**": true,
    "**/yarn.lock": true
  },
  "http.proxyAuthorization": null,
  "npm.packageManager": "yarn",
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.fontSize": 16,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.arrowParens": "always",
  "prettier.endOfLine": "lf",
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": true,
  "git.enableSmartCommit": true,
  "gitlens.advanced.messages": { "suppressShowKeyBindingsNotice": true },
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "code-runner.runInTerminal": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.compile.nullAnalysis.mode": "automatic",
  "java.jdt.ls.java.home": "D:\\Program Files\\Java\\jdk-20",
  "java.configuration.maven.globalSettings": "D:\\Program Files\\Java\\apache-maven-3.9.6\\conf\\settings.xml",
  "java.configuration.maven.userSettings": "D:\\Program Files\\Java\\apache-maven-3.9.6\\conf\\settings.xml",
  "maven.executable.path": "D:\\Program Files\\Java\\apache-maven-3.9.6\\bin\\mvn",
  "rsp-ui.rsp.java.home": "D:\\Program Files\\Java\\jdk-20",
  "rsp-ui.enableStartServerOnActivation": [
    {
      "id": "redhat.vscode-community-server-connector",
      "name": "Community Server Connector",
      "startOnActivation": true
    }
  ],
  "redhat.telemetry.enabled": false,
  "typescript.check.npmIsInstalled": false,
  "vetur.format.defaultFormatterOptions": {
    "prettier": { "singleQuote": true }
  },
  "wxmlConfig.format": { "indent_with_tabs": false, "indent_char": " " },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[java]": { "editor.defaultFormatter": "mwpb.java-prettier-formatter" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
