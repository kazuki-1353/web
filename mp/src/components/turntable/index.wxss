:host {
  color: #ffffff;
}

.comp {
  position: relative;
  text-align: center;
}

.outside {
  position: relative;
  z-index: 1;
  width: 624rpx;
  height: 624rpx;
  vertical-align: middle;
}
.inside {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 2;
  margin: auto;
  width: 200rpx;
  height: 200rpx;
  vertical-align: middle;
}
.inside-center {
  display: inline-flex;
  flex-direction: column;
  justify-content: center;
  width: 200rpx;
  height: 200rpx;
  font-weight: bold;
}
.inside-center__text {
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #641706;
}
.inside-center__num {
  font-size: 40rpx;
  color: #5c0f00;
}
.main {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 576rpx;
  height: 576rpx;
  vertical-align: middle;
}
