.fixe {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
}
.box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 80rpx 0;
  height: 100%;
}
.wrap {
  position: relative;
  border-radius: 10rpx;
  overflow: auto;
}
.icon {
  position: absolute;
  top: 0;
  right: 0;
  padding: 40rpx;
  width: 40rpx;
  height: 40rpx;
}

@import '../../static/animate.min.wxss';
